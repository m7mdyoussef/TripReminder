<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.tripreminder2021.ui.navigation.upComing.UpcomingFragment"
        android:label="UpComing Trips"
        tools:layout="@layout/fragment_home">
    </fragment>
    <fragment
        android:id="@+id/nav_history"
        android:name="com.example.tripreminder2021.ui.navigation.history.HistoryFragment"
        android:label="@string/home_second"
        tools:layout="@layout/fragment_history">
    </fragment>

    <fragment
        android:id="@+id/nav_report"
        android:name="com.example.tripreminder2021.ui.navigation.report.ReportFragment"
        android:label="@string/Reports"
        tools:layout="@layout/fragment_reports" />


</navigation>